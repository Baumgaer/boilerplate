{
    "name": "boilerplate",
    "description": "Building web apps and mobile apps decoupled from data with one code base as fullstack",
    "version": "0.0.1",
    "private": true,
    "engineStrict": true,
    "engines": {
        "npm": "~8.18.0",
        "node": ">= 16.16.0"
    },
    "scripts": {
        "dev": "npm-run-all -p dev:!(app)",
        "dev:server": "echo 'no dev:server script'",
        "dev:web": "vue-cli-service serve --env development --deep-monitoring --source-map-support",
        "//1": "",
        "dev:app": "npm run build:web && cap sync && cap run",
        "dev:app:android": "npm run build:web && cap sync && cap run android",
        "dev:app:ios": "npm run build:web && cap sync && cap run ios",
        "//2": "",
        "build": "npm-run-all -p build:!(app)",
        "build:server": "echo 'no build:server script'",
        "build:web": "vue-cli-service build",
        "//3": "",
        "build:app": "npm run build:web && cap copy && cap sync && cap open",
        "build:app:android": "npm run build:web && cap copy && cap sync && cap open android",
        "build:app:ios": "npm run build:web && cap copy && cap sync && cap open ios",
        "//4": "",
        "test": "npm run test:unit && npm run test:e2e",
        "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "//5": "",
        "lint": "npm-run-all -p lint:*",
        "lint:script": "vue-cli-service lint",
        "lint:style": "stylelint **/*.scss",
        "//6": "",
        "type-coverage": "typescript-coverage-report -o ./coverage/type-report",
        "prepare": "cd dev/eslint-plugin-boilerplate && npm run build && cd ../.. && is-ci || husky install"
    },
    "dependencies": {
        "@babel/runtime": "^7.18.9",
        "@capacitor/android": "4.0.1",
        "@capacitor/app": "4.0.1",
        "@capacitor/core": "4.0.1",
        "@capacitor/haptics": "4.0.1",
        "@capacitor/ios": "4.0.1",
        "@capacitor/keyboard": "4.0.1",
        "@capacitor/status-bar": "4.0.1",
        "@ionic/pwa-elements": "^3.1.1",
        "@ionic/vue": "^6.2.1",
        "@ionic/vue-router": "^6.2.1",
        "app-root-path": "^3.0.0",
        "compression": "^1.7.4",
        "core-js": "^3.22.5",
        "cross-env": "^7.0.3",
        "csurf": "^1.11.0",
        "deepdash": "^5.3.9",
        "deepdash-es": "^5.3.9",
        "express": "^4.18.1",
        "express-session": "^1.17.3",
        "file-type": "^17.1.6",
        "helmet": "^5.1.1",
        "hpp": "^0.2.3",
        "http-errors": "^2.0.0",
        "i18next": "^21.9.0",
        "i18next-browser-languagedetector": "^6.1.4",
        "i18next-http-middleware": "^3.2.1",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "ms": "^2.1.3",
        "normalize-url": "^7.0.3",
        "on-change": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "sql.js": "^1.7.0",
        "tslib": "^2.4.0",
        "typeorm": "^0.3.7",
        "uuid": "^8.3.2",
        "vue": "^3.2.37",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-property-decorator": "^10.0.0-rc.3",
        "vue-router": "^4.1.3",
        "yaml": "^2.1.1",
        "zod": "^3.17.10"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.18.10",
        "@capacitor/cli": "4.0.1",
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@ionic/cli": "^6.20.1",
        "@types/app-root-path": "^1.2.4",
        "@types/chai": "^4.3.3",
        "@types/compression": "^1.7.2",
        "@types/csurf": "^1.11.2",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.5",
        "@types/hpp": "^0.2.2",
        "@types/http-errors": "^1.8.2",
        "@types/lodash": "^4.14.184",
        "@types/mocha": "^9.1.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^18.7.8",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.33.1",
        "@typescript-eslint/parser": "^5.33.1",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-e2e-cypress": "^5.0.8",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-plugin-router": "^5.0.8",
        "@vue/cli-plugin-typescript": "^5.0.8",
        "@vue/cli-plugin-unit-mocha": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.0.2",
        "babel-plugin-istanbul": "^6.1.1",
        "chai": "^4.3.6",
        "cypress": "^10.6.0",
        "eslint": "^8.22.0",
        "eslint-import-resolver-typescript": "^3.4.2",
        "eslint-plugin-boilerplate": "./dev/eslint-plugin-boilerplate",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-vue": "^9.3.0",
        "husky": "^8.0.1",
        "is-ci": "^3.0.1",
        "lint-staged": "^13.0.3",
        "native-run": "^1.6.0",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "sass": "^1.54.5",
        "sass-loader": "^13.0.2",
        "stylelint": "^14.10.0",
        "stylelint-config-clean-order": "^0.9.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "ts-plugin-type-coverage": "^2.22.0",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "ttypescript": "^1.5.13",
        "type-coverage": "^2.22.0",
        "type-fest": "^2.18.1",
        "typescript": "^4.7.4",
        "typescript-coverage-report": "^0.6.4",
        "webpack": "^5.74.0",
        "yaml-loader": "^0.8.0"
    }
}
