{
    "name": "boilerplate",
    "description": "Building web apps and mobile apps decoupled from data with one code base as fullstack",
    "version": "0.0.1",
    "private": true,
    "engineStrict": true,
    "engines": {
        "npm": ">=8.18.0",
        "node": ">= 16.16.0"
    },
    "bin": "bin/index.js",
    "scripts": {
        "start": "nodemon ./dist/server/server.js",
        "start:dev": "nodemon -w ./dist/server --delay --exec --inspect --enable-source-maps ./dist/server/server.js",
        "dev": "npm-run-all -p dev:!(app)",
        "dev:server": "npm run compile:build:client:config && concurrently \"npm run compile:dev:server:config\" \"webpack --mode development --watch --config webpack.config.server.js\"",
        "dev:web": "npm run compile:build:client:config && concurrently \"npm run compile:dev:client:config\" \"vue-cli-service serve --env development --deep-monitoring --source-map-support\"",
        "//1": "",
        "dev:app": "npm run build:web && cap sync && cap run",
        "dev:app:android": "npm run build:web && cap sync && cap run android -l --external --watch",
        "dev:app:ios": "npm run build:web && cap sync && cap run ios -l --external --watch",
        "//1b": "",
        "compile:dev:server:config": "cross-env NODE_ENV=development TS_CONFIG_PATH=src/server/tsconfig.json ENVIRONMENT=server gulp",
        "compile:dev:client:config": "cross-env NODE_ENV=development TS_CONFIG_PATH=src/client/tsconfig.json ENVIRONMENT=client gulp",
        "//2": "",
        "build": "npm-run-all -p build:!(app)",
        "build:server": "npm run compile:build:server:config && webpack --mode production --config webpack.config.server.js",
        "build:web": "npm run compile:build:client:config && vue-cli-service build",
        "//2b": "",
        "compile:build:server:config": "cross-env NODE_ENV=production TS_CONFIG_PATH=src/server/tsconfig.json ENVIRONMENT=server gulp",
        "compile:build:client:config": "cross-env NODE_ENV=production TS_CONFIG_PATH=src/client/tsconfig.json ENVIRONMENT=client gulp",
        "//3": "",
        "build:app": "npm run build:web && cap copy && cap sync && cap open",
        "build:app:android": "npm run build:web && cap copy && cap sync && cap open android",
        "build:app:ios": "npm run build:web && cap copy && cap sync && cap open ios",
        "//4": "",
        "test": "npm run test:unit && npm run test:e2e",
        "test:unit": "npm run test:unit:server && npm run test:unit:client",
        "test:unit:server": "echo \"no test:unit:server script\"",
        "test:unit:client": "npm run compile:test:unit:client:config && cross-env NODE_ENV=test nyc vue-cli-service test:unit ./tests/unit/client/index.spec.ts",
        "test:e2e": "vue-cli-service test:e2e",
        "//4b": "",
        "compile:test:unit:server:config": "cross-env NODE_ENV=test TS_CONFIG_PATH=tests/unit/server/tsconfig.json ENVIRONMENT=server gulp",
        "compile:test:unit:client:config": "cross-env NODE_ENV=test TS_CONFIG_PATH=tests/unit/client/tsconfig.json ENVIRONMENT=client gulp",
        "//5": "",
        "lint": "npm-run-all -p lint:*",
        "lint:script": "vue-cli-service lint",
        "lint:style": "stylelint **/*.scss",
        "//6": "",
        "type-coverage": "npm-run-all -s type-coverage:* && npm-run-all -s type-coverage:test:*:*",
        "type-coverage:prepare": "is-ci || make-dir ./coverage/type-report/client && make-dir ./coverage/type-report/server && make-dir ./coverage/type-report/tests/unit/client && make-dir ./coverage/type-report/tests/unit/server",
        "type-coverage:client": "is-ci && type-coverage -p ./src/client/tsconfig.json --at-least 95 || typescript-coverage-report -p ./src/client/tsconfig.json -o ./coverage/type-report/client",
        "type-coverage:server": "is-ci && type-coverage -p ./src/server/tsconfig.json --at-least 95 || typescript-coverage-report -p ./src/server/tsconfig.json -o ./coverage/type-report/server",
        "type-coverage:test:unit:client": "is-ci && type-coverage -p ./tests/unit/client/tsconfig.json --at-least 95 || typescript-coverage-report -p ./tests/unit/client/tsconfig.json -o ./coverage/type-report/tests/unit/client",
        "type-coverage:test:unit:server": "is-ci && type-coverage -p ./tests/unit/server/tsconfig.json --at-least 95 || typescript-coverage-report -p ./tests/unit/server/tsconfig.json -o ./coverage/type-report/tests/unit/server",
        "//7": "",
        "prepare": "npm-run-all -s prepare:*",
        "prepare:eslint-plugin-boilerplate": "cd ./dev/eslint-plugin-boilerplate && npm ci && npm run build",
        "prepare:typescript-transformer-boilerplate": "cd ./dev/typescript-transformer-boilerplate && npm ci && npm run build",
        "prepare:husky": "is-ci || husky install",
        "prepare:ts-patch": "ts-patch install"
    },
    "dependencies": {
        "@babel/runtime": "^7.22.6",
        "@capacitor/android": "^5.2.1",
        "@capacitor/app": "5.0.3",
        "@capacitor/core": "^5.2.1",
        "@capacitor/haptics": "5.0.4",
        "@capacitor/ios": "^5.2.1",
        "@capacitor/keyboard": "5.0.4",
        "@capacitor/status-bar": "5.0.4",
        "@ionic/pwa-elements": "^3.2.1",
        "@ionic/vue": "^7.1.3",
        "@ionic/vue-router": "^7.1.3",
        "app-root-path": "^3.1.0",
        "compression": "^1.7.4",
        "core-js": "^3.31.1",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "deepdash": "^5.3.9",
        "deepdash-es": "^5.3.9",
        "express": "^4.18.2",
        "express-query-parser": "^1.3.3",
        "express-session": "^1.17.3",
        "file-type": "^18.5.0",
        "fs-extra": "^11.1.1",
        "helmet": "^7.0.0",
        "hpp": "^0.2.3",
        "http-errors": "^2.0.0",
        "i18next": "^23.2.11",
        "i18next-browser-languagedetector": "^7.1.0",
        "i18next-http-middleware": "^3.3.2",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "mkdirp": "^3.0.1",
        "ms": "^2.1.3",
        "normalize-url": "^8.0.0",
        "on-change": "^4.0.2",
        "path-browserify": "^1.0.1",
        "pg": "^8.11.1",
        "pg-native": "^3.0.1",
        "process": "^0.11.10",
        "reflect-metadata": "^0.1.13",
        "setimmediate": "^1.0.5",
        "sql.js": "^1.8.0",
        "triple-beam": "^1.4.1",
        "tslib": "^2.6.0",
        "typeorm": "0.3.12",
        "util": "^0.12.5",
        "uuid": "^9.0.0",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4",
        "winston": "^3.10.0",
