{
    "basePath": "src",
    "entryPoints": [
        "src/client/",
        "src/common/"
    ],
    "entryPointStrategy": "expand",
    "hideGenerator": true,
    "categorizeByGroup": true,
    "cleanOutputDir": true,
    "includeVersion": true,
    "excludeNotDocumented": true,
    "tsconfig": "src/client/tsconfig.json",
    "out": "docs/",
    "theme": "hierarchy",
    "exclude": [
        "./node_modules/typeorm/repository/BaseEntity.d.ts"
    ],
    "navigationLinks": {
        "GitHub": "https://github.com/Baumgaer/boilerplate"
    },
    "umlClassDiagram": {
        "type": "simple",
        "location": "remote",
        "format": "svg"
    }
}
